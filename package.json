{
  "name": "express-api-starter",
  "version": "1.2.0",
  "description": " A basic starter for an express.js API",
  "main": "server.js",
  "scripts": {
     "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start": "node server.js",
    "client": "cd client and npm run start",
    "dev": "nodemon backend/src/index.js",
    "build": "cd client && npm run build",
    "lint": "eslint --fix src",
    "audit": "snyk test",
    "test": "mocha -w",
    "install": "cd client and npm install",
     "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "keywords": [],
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willcofer555/traildoxx.git"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs": "^3.0.1",
    "express": "^4.16.4",
    "gridfs-stream": "^1.1.1",
    "helmet": "^3.21.2",
    "joi": "^14.3.1",
    "method-override": "^3.0.0",
    "monk": "^7.1.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.1",
    "now": "^16.7.0",
    "react-scripts": "^3.3.0"
  },
  "devDependencies": {
    "bootstrap": "^4.4.1",
    "ajv": "^6.10.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "leaflet": "^1.6.0",
    "mocha": "^5.2.0",
    "mongodb": "^3.4.1",
    "nodemon": "^1.18.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "snyk": "^1.254.0",
    "supertest": "^3.3.0",
    "typescript": "^3.7.3",
    "react-leaflet": "^2.6.1",
    "reactstrap": "^8.2.0"
  },
  "bugs": {
    "url": "https://github.com/w3cj/express-api-starter/issues"
  },
  "homepage": "https://github.com/w3cj/express-api-starter#readme",
  "directories": {
    "test": "test"
  },
  "cacheDirectories": [
    "client/node_modules",
    "backend/node_modules"
  ]
}
